<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>mdryden.cflapi.v1</AssemblyName>
    <RootNamespace>mdryden.cflapi.v1</RootNamespace>
    <PackageId>mdryden.cflapi.v1</PackageId>
    <Version>3.1.25.1</Version>
    <Authors>Michael Dryden</Authors>
    <Company>Michael Dryden</Company>
    <Product>cfl-api.net</Product>
    <Description>.Net implementation of the Canadian Football League's public API</Description>
    <Copyright>Copyright Michael Dryden © 2017</Copyright>
    <RepositoryUrl>https://github.com/pudds/cfl-api.net</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>